<Window x:Class="EditorGuiApp.EditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EditorGuiApp"
        mc:Ignorable="d"
            Title="EditName" Height="160" Width="260">
    <Grid Margin="0,0,0,-6">
        <DatePicker x:Name="Date" HorizontalAlignment="Left" Height="25" Margin="71,50,0,0" VerticalAlignment="Top" Width="168"/>
        <TextBox x:Name="Name"  HorizontalAlignment="Left" Height="26" Margin="71,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="168"/>
        <Button Content="Cancel" Margin="171,92,15,18" Click="Cancel_Button_Click" />
        <TextBlock HorizontalAlignment="Left" Margin="20,15,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="20,54,0,0" TextWrapping="Wrap" Text="Date" VerticalAlignment="Top"/>
        <Button Content="OK" Margin="87,92,99,18"  Click="OK_Button_Click" />
    </Grid>
</Window>
